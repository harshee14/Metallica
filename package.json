{
  "name": "Metallica",
  "version": "1.0.0",
  "description": "Metallica App.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "start-trade": "cd src/standalone-services/TradeService && npm start",
    "start-marketdata": "cd src/standalone-services/MarketdataService && npm start",
    "start-refdata": "cd src/standalone-services/RefDataService && npm start",
    "start-notification": "cd src/standalone-services/NotificationService && npm start",
    "start-micro-services": "concurrently \"npm run start-trade\" \"npm run start-marketdata\" \"npm run start-refdata\" \"npm run start-notification\"",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently 'npm run server' 'npm run client'"
  },
  "author": "Harshita Bhatnagar",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "log4js": "^3.0.5",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.5.2",
    "superagent": "^4.0.0-beta.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
